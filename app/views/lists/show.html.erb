<h1>Show List</h1>

<h2><%= @list.name %></h2>

<ul>
  <% @bookmarks.each do |bk| %>
    <li>
      <%= bk.comment %>
    </li>
  <% end %>
</ul>

<%= link_to 'Back Lists', lists_path, class: 'btn btn-secondary' %>
<%= link_to 'Add Bookmark', new_list_bookmark_path(@list), class: 'btn btn-secondary' %>
